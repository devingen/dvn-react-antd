{
  "name": "dvn-antd",
  "version": "0.1.12",
  "description": "Devingen form implementation with antd framework.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar --include-path ./lib --include-path ./node_modules lib/ -o dist/",
    "build-js": "tsc",
    "test": "npm-run-all build-css-test test-js",
    "build-css-test": "node-sass-chokidar --include-path ./lib --include-path ./node_modules lib/ -o lib/",
    "test-js": "jest --coverage",
    "jest": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devingen/js-antd.git"
  },
  "author": "eluleci",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/devingen/js-antd/issues"
  },
  "homepage": "https://github.com/devingen/js-antd#readme",
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts"
    ],
    "testMatch": [
      "<rootDir>/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/**/+(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.9",
    "@types/prop-types": "^15.5.6",
    "@types/react": "^16.7.1",
    "@types/react-dom": "^16.0.9",
    "babel-jest": "^23.6.0",
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "jest": "^23.6.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "ts-jest": "^23.10.4",
    "tslib": "^1.9.3",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "antd": "^3.10.4",
    "classnames": "^2.2.6",
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  }
}
